    <DataSet Name="DSDbInfo">
      <Query>
        <DataSourceName>LEAVE</DataSourceName>
        <CommandText>SET DATEFIRST 1;SELECT
(SELECT        SYSTEM_USER AS u) AS [User Id],
CONVERT(VARCHAR(10),GETDATE(),120) as Today,
CONVERT(VARCHAR(10),(SELECT DATEADD(DAY, 1 - DATEPART(WEEKDAY, DATEADD(WEEK,-1,GETDATE())), CAST(DATEADD(WEEK,-1,GETDATE()) AS DATE))),120) WeekStart,
CONVERT(VARCHAR(10),(SELECT DATEADD(DAY, 7 - DATEPART(WEEKDAY, DATEADD(WEEK,-1,GETDATE())), CAST(DATEADD(WEEK,-1,GETDATE()) AS DATE))),120) WeekEnd</CommandText>
      </Query>
