  <Code>Public Function ConvertMinutesToString(ByVal minutes As Integer, ByVal QUOTA_UNLIMITED As Integer, ByVal HOURSPERDAY As Double) As String
  If (Minutes=0 Or Minutes = QUOTA_UNLIMITED) Then Return "-"

  Dim ahmins As Integer = 60
  Dim admins As Double = ahmins * HOURSPERDAY
  Dim tomins As Double = Abs(minutes)
  Dim f As Integer = 1
  If minutes &lt; 0 Then f = -1
  Dim d As Integer = Floor(Abs(minutes / admins))
  Dim h As Integer = Floor((tomins Mod admins) / ahmins)
  Dim m As Integer = Floor((tomins Mod admins) Mod ahmins)

  Dim s As String = CStr(d*f)+":"+Right("0"+CStr(h),2)+":"+Right("0"+CStr(m),2)
'  If m=0 Then
'    s = CStr(d*f)+":"+Right("0"+CStr(h),2)
'  ElseIf h=0 And m=0 Then
'    s = CStr(d*f)
'  Else
'    s = CStr(d*f)+":"+Right("0"+CStr(h),2)+":"+Right("0"+CStr(m),2)
'  End If
  Return s
End Function

Public Function JoinWithStatusName(ByVal statuses As String, ByVal dsStatuses As Object()) As String
  Dim i As Integer
  Dim statusID As String
  Dim statusName As String
  Dim arrIn() As String = Split(statuses, ",")
  Dim l As Integer = arrIn.Length
  Dim arrOut() As String
  Dim arrStr(2) As String
  For i = 0 To arrIn.Length - 1
    statusID = arrIn(i)
    statusName = ""
    If Not dsStatuses Is Nothing Then
      For Each txt As Object In dsStatuses
        arrStr = Split(CStr(txt),"|")
        If statusID = arrStr(LBound(arrStr)) Then
          statusName = arrStr(UBound(arrStr))
          Exit For
        End If
      Next
    End If
    ReDim Preserve arrOut(i)
    arrOut(i) = CStr(statusID) + " - " + CStr(statusName)
  Next
  Dim s As String = "[" + Join(arrOut, "], [") + "]"
  Return s
End Function</Code>
