    <DataSet Name="DSDepartment">
      <Query>
        <DataSourceName>LEAVE</DataSourceName>
        <CommandText>select distinct [Company Code]+Department+Section as Code,(case when Department='' then '-' else Department end)+' &gt; '+(case when Section='' then '-' else Section end) as Department
from [HR Employee] with(readuncommitted)
where DATEADD(dd, 0, DATEDIFF(dd, 0, GETDATE())) between [Starting Date] and [Until Date]
order by [Company Code]+Department+Section</CommandText>
