  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>LEAVE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dt1">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dt2">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@em">
            <Value>=Parameters!Employee.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@com">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dep">
            <Value>=Parameters!Department.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@active">
            <Value>=Parameters!ShowActiveOnly.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with drange as (
	select dateadd(dd, 0, @dt1) as dt
	union all
	select dateadd(dd,1,dt) from drange
	where dateadd(dd,1,dt) &lt;= @dt2 and @dt1 &lt;= @dt2
)
select emp.[Company Code]
,emp.[Department]
,emp.[Section]
,emp.[Employee No]
,per.[TH Prefix]
,per.[TH First Name]
,per.[TH Last Name]
,dr.dt as [Date]
,1 as [Expected Work Day]
,cast(1-isnull(lev.[Days],0) as float) as [Actual Work Day]
,cast(case lev.[Type Sub ID] when 1 then isnull(lev.[Days],0) else 0 end as float) as [Leave Sick Day]
,cast(case lev.[Type Sub ID] when 2 then isnull(lev.[Days],0) else 0 end as float) as [Leave Business Day]
,cast(case lev.[Type Sub ID] when 3 then isnull(lev.[Days],0) else 0 end as float) as [Leave Vacation Day]
,cast(case when lev.[Type Sub ID] not in (1,2,3) then isnull(lev.[Days],0) else 0 end as float) as [Leave Other Day]
from drange dr
inner join [HR Person] per with(readuncommitted) on 1=1
inner join [HR Employee] emp with(readuncommitted) on per.[Person No]=emp.[Person No]
inner join (
	select [Person No], min([Employee No]) AS [Employee No], max([Starting Date]) AS [Starting Date]
	from [HR Employee] with(readuncommitted)
	group by [Person No]
) lem on emp.[Person No]=lem.[Person No]
and emp.[Employee No]=lem.[Employee No]
and emp.[Starting Date]=lem.[Starting Date]
left outer join (
	select [Leave Date],[Person No],[Type Sub ID],sum([Approve Days]) as [Days]
	from [LV Leave] with(readuncommitted)
	where [Leave Date] between @dt1 and @dt2 and [Status ID] in (12,14,103)
	group by [Leave Date],[Person No],[Type Sub ID]
) lev on dr.dt=lev.[Leave Date] and emp.[Person No]=lev.[Person No]
where upper(replace(emp.[Employee No],'-',''))=isnull(upper(replace(@em,'-','')),upper(replace(emp.[Employee No],'-','')))
and emp.[Company Code] in (@com)
and emp.[Company Code]+emp.[Department]+emp.[Section] in (@dep)
and (@active&lt;&gt;1 OR DATEADD(dd, 0, DATEDIFF(dd, 0, GETDATE())) between emp.[Starting Date] and emp.[Until Date])
order by emp.[Company Code]
,emp.[Department]
,emp.[Section]
,emp.[Employee No]
,dr.dt
option(maxrecursion  0)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
