    <DataSet Name="DSDbInfo">
      <Query>
        <DataSourceName>LEAVE</DataSourceName>
        <CommandText>SET DATEFIRST 1;SELECT
(SELECT        SYSTEM_USER AS u) AS [User Id],
CONVERT(VARCHAR(10),GETDATE(),120) as Today,
CONVERT(VARCHAR(10),(SELECT DATEADD(DAY, 1 - DATEPART(WEEKDAY, DATEADD(WEEK,-1,GETDATE())), CAST(DATEADD(WEEK,-1,GETDATE()) AS DATE))),120) WeekStart,
CONVERT(VARCHAR(10),(SELECT DATEADD(DAY, 7 - DATEPART(WEEKDAY, DATEADD(WEEK,-1,GETDATE())), CAST(DATEADD(WEEK,-1,GETDATE()) AS DATE))),120) WeekEnd,
CONVERT(VARCHAR(10),DATEADD(MM, DATEDIFF(MM, 0, GETDATE()), 0),120) MonthStart,
CONVERT(VARCHAR(10),DATEADD(DD,-DAY(DATEADD(MM,1,GETDATE())),DATEADD(MM,1,GETDATE())),120) MonthEnd,
CASE WHEN DAY(GETDATE())&lt;=15 THEN CONVERT(VARCHAR(10),DATEADD(MM, DATEDIFF(MM, 0, GETDATE()), 0),120)
ELSE CONVERT(VARCHAR(10),DATEADD(MM, DATEDIFF(MM, 0, GETDATE()), 14),120) END PayrollStart,
CASE WHEN DAY(GETDATE())&lt;=15 THEN CONVERT(VARCHAR(10),DATEADD(MM, DATEDIFF(MM, 0, GETDATE()), 14),120)
ELSE CONVERT(VARCHAR(10),DATEADD(DD,-DAY(DATEADD(MM,1,GETDATE())),DATEADD(MM,1,GETDATE())),120) END PayrollEnd</CommandText>
      </Query>
