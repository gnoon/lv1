	@{
		var ContentsRequired = new ViewDataDictionary {
		{"base.ui",true},{"jquery",true},{"jquery.ui",true},{"jquery.ui.core",true},
		{"jquery.ui.widget",true},{"jquery.ui.button",true},{"jquery.ui.tabs",true},
